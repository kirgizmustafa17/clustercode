<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="Unit Tests" type="JUnit" factoryName="JUnit">
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="net.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <option name="PACKAGE_NAME" value="net" />
    <option name="MAIN_CLASS_NAME" value="" />
    <option name="METHOD_NAME" value="" />
    <option name="TEST_OBJECT" value="pattern" />
    <option name="VM_PARAMETERS" value="-ea -Dorg.apache.logging.log4j.simplelog.StatusLogger.level=INFO" />
    <option name="PARAMETERS" value="" />
    <option name="WORKING_DIRECTORY" value="$MODULE_DIR$" />
    <option name="TEST_SEARCH_SCOPE">
      <value defaultName="wholeProject" />
    </option>
    <patterns>
      <pattern testClass="^(?!.*IT$).*$" />
    </patterns>
    <method v="2">
      <option name="Make" enabled="true" />
    </method>
  </configuration>
</component>